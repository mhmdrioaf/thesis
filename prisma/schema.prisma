generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String    @id
  username      String?   @unique
  password      String?
  name          String?
  email         String?   @unique
  emailVerified DateTime?
  image         String?
}
